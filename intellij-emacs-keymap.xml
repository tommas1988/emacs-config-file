<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="AddToFavoritesPopup" />
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl p" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl c" />
  </action>
  <action id="EditorScrollBottom">
    <keyboard-shortcut first-keystroke="shift alt period" />
  </action>
  <action id="EditorScrollTop">
    <keyboard-shortcut first-keystroke="shift alt comma" />
  </action>
  <action id="EditorTextEnd">
    <keyboard-shortcut first-keystroke="ctrl end" />
  </action>
  <action id="EditorTextStart">
    <keyboard-shortcut first-keystroke="ctrl home" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="comma" />
  </action>
  <action id="FindInPath">
    <keyboard-shortcut first-keystroke="shift ctrl f" />
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl n" />
  </action>
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="shift alt g" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="c" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl alt g" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="period" />
    <keyboard-shortcut first-keystroke="alt period" />
    <mouse-shortcut keystroke="control button1" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl d" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl i" />
  </action>
  <action id="GotoSuperMethod">
    <keyboard-shortcut first-keystroke="ctrl u" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl s" />
  </action>
  <action id="GotoSymbol">
    <keyboard-shortcut first-keystroke="shift ctrl alt n" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="s" />
  </action>
  <action id="HideAllWindows">
    <keyboard-shortcut first-keystroke="shift ctrl f12" />
    <keyboard-shortcut first-keystroke="f12" />
  </action>
  <action id="JumpToLastChange">
    <keyboard-shortcut first-keystroke="shift ctrl back_space" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="l" />
  </action>
  <action id="JumpToLastWindow" />
  <action id="JumpToNextChange">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="n" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="shift alt f" />
  </action>
</keymap>